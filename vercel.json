{
  "rewrites": [
    {"source": "/slimmatica", "destination": "/slimmatica/index.html"},
    {"source": "/glucostra", "destination": "/glucostra/index.html"}
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {"key": "X-Frame-Options", "value": "ALLOWALL"},
        {"key": "Content-Security-Policy", "value": "frame-ancestors *"}
      ]
    }
  ]
}
